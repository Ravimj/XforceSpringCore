<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="person" class="com.dev.spring.core.Person">
		<property name="name" value="Ryder"/>
		<property name="job" ref="developer" />
		<property name="email" value="ryder@mail.com"/>
	</bean>
	
	<bean id="developer" class="com.dev.spring.core.SoftwareDeveloper">
	</bean>
	
	<bean id="tester" class="com.dev.spring.core.SoftwareTester"/>
</beans>